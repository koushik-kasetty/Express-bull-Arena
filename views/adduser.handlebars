<h1>Add Transactions</h1>
{{#if error}} <span>{{error}}</span>{{/if}}
<form method="POST" action="/user/add">
  <div class="form-group">
    <label>Transaction Id</label>
    <input type="text" name="id" class="form-control" placeholder="Add ID">
  </div>
  <div class="form-group">
    <label>Enterprise ID </label>
    <input type="text" name="enterprise_id" class="form-control" placeholder="Enterprise ID">
  </div>
  <div class="form-group">
    <label>Product Instance Id </label>
    <input type="text" name="instance_id" class="form-control" placeholder="Instace Id">
  </div>
  <div class="form-group">
    <label>Email</label>
    <input type="text" name="email" class="form-control" placeholder="Email Address">
  </div>
  <div class="form-group">
    <label>Duration</label>
    <input type="text" name="time" class="form-control" placeholder="In Millseconds">
  </div>
  <div class="form-group">
    <label>state</label>
    <input type="text" name="state" class="form-control" placeholder="state">
  </div>

  <input type="submit" class="btn btn-primary" value="Save">
</form>
